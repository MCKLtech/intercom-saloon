{
  "name": "mckltech/intercom-saloon",
  "description": "PHP Library for the Intercom REST API using Saloon",
  "authors": [
    {
      "name": "Colin Longworth",
      "email": "info@wooninja.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "saloonphp/saloon": "^3.0",
    "saloonphp/pagination-plugin": "^2.2",
    "saloonphp/rate-limit-plugin": "^2.0",
    "nesbot/carbon": "^2.72.1",
    "illuminate/collections": "^10.46"
  },
  "require-dev": {
    "spatie/ray": "*"
  },
  "autoload": {
    "psr-4": {
      "WooNinja\\IntercomSaloon\\": "src/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable" : true
}
